<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Rapport.
    -->
    <changeSet id="20190626132939-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="projet_id"
                                 baseTableName="rapport"
                                 constraintName="fk_rapport_projet_id"
                                 referencedColumnNames="id"
                                 referencedTableName="projet"/>

        <addForeignKeyConstraint baseColumnNames="redacteur_id"
                                 baseTableName="rapport"
                                 constraintName="fk_rapport_redacteur_id"
                                 referencedColumnNames="id"
                                 referencedTableName="personne"/>

    </changeSet>
</databaseChangeLog>
